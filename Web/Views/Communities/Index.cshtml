@model Community[]
<div class="row">

@foreach(var community in @Model)
{
	<div class="col s12 m6 l4">
	<div class="card">
		<div class="card-image">
			<img src='@community.picture'>
			<a href='@Url.Action("Profile", "Communities")?id=@community.id' class="btn-floating halfway-fab waves-effect waves-light teal" style="font-size:.7rem; text-align: center;">Join</a>
		</div>
		<div class="card-content">
			<a href='@Url.Action("Profile", "Communities")?id=@community.id' class="card-title" style="padding-bottom: 5px; border-bottom: 1px solid #ee6e73;" >@community.title</a>
			<!--<small><strong>Members: </strong>293</small>-->
		</div>
	</div>
</div>
}
	
	
